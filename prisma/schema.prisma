generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type ProjectLinks {
  label String
  url   String
}

model Project {
  id          String         @id @default(auto()) @map("_id") @db.ObjectId
  cover       String
  createdAt   DateTime       @db.Date
  description String
  /// Could not determine type: the field only had null or empty values in the sample set.
  endDate     Json?
  featured    Boolean
  /// Could not determine type: the field only had null or empty values in the sample set.
  gallery     Json?
  links       ProjectLinks[]
  slug        String         @unique
  /// Could not determine type: the field only had null or empty values in the sample set.
  startDate   Json?
  status      String
  summary     String
  tags        String[]
  /// Could not determine type: the field only had null or empty values in the sample set.
  techStack   Json?
  title       String
  updatedAt   DateTime       @db.Date
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  /// Field referred in an index, but found no data to define the type.
  email Json?  @unique
}

model projects {
  id               String    @id @default(auto()) @map("_id") @db.ObjectId
  client           String?
  createdAt        DateTime  @db.Date
  demoUrl          String?
  description      String
  endDate          DateTime? @db.Date
  featuredImage    String
  gallery          String[]
  industry         String
  isPublic         Boolean
  problemStatement String
  slug             String
  solution         String
  startDate        DateTime  @db.Date
  status           String
  techStack        String[]
  title            String
  updatedAt        DateTime  @db.Date
}
